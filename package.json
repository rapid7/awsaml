{
  "name": "awsaml",
  "version": "1.5.0",
  "description": "Periodically refreshes AWS access keys",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rapid7/awsaml.git"
  },
  "bugs": {
    "url": "https://github.com/rapid7/awsaml/issues"
  },
  "main": "app.js",
  "scripts": {
    "electron-debug": "electron . --debug",
    "electron-start": "electron .",
    "local-start": "node local.js",
    "prebuild": "rm -rf dist/",
    "build": "export PLATFORM=${PLATFORM:=darwin,linux,win32}; electron-packager . Awsaml --asar --ignore=node_modules/electron --ignore=test --ignore=dist --out=dist --platform=$PLATFORM --arch=x64 --app-version=${npm_package_version} --app-bundle-id=com.rapid7.awsaml --helper-bundle-id=com.rapid7.awsaml.helper",
    "postbuild": "for platform in `echo $PLATFORM | sed 's/,/ /g'`; do export platform=$platform; npm run zip; done",
    "zip": "cd dist/Awsaml-${platform}-x64 && zip -q -FS -r ../awsaml-v${npm_package_version}-${platform}-x64.zip .",
    "test": "mocha",
    "cover": "rm -Rf coverage/* && istanbul cover _mocha -- -R spec",
    "lint": "eslint *.js views/** lib/** test/**",
    "report": "coveralls < ./coverage/lcov.info",
    "watch": "webpack-dev-server",
    "pack": "webpack --progress --colors --config=webpack.config.pack.js",
    "show-appcast-checkpoint": "curl --compressed --location --user-agent 'Mozilla/5.0 (Macintosh; Intel Mac OS X 10_9_2) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/33.0.1750.152 Safari/537.36' 'https://github.com/rapid7/awsaml/releases.atom' | /usr/bin/sed 's|<pubDate>[^<]*</pubDate>||g' | shasum --algorithm 256"
  },
  "dependencies": {
    "aws-sdk": "2.2.11",
    "body-parser": "1.14.1",
    "cookie-parser": "1.4.0",
    "electron": "^1.6.11",
    "electron-packager": "^8.7.0",
    "errorhandler": "1.4.2",
    "express": "4.13.3",
    "express-react-views": "^0.10.0",
    "express-session": "1.13.0",
    "ini": "1.3.4",
    "mkdirp": "^0.5.1",
    "morgan": "1.6.1",
    "passport": "0.3.0",
    "passport-saml": "0.13.0",
    "prop-types": "^15.5.10",
    "react": "^15.1.0",
    "react-dom": "^15.5.4",
    "xmldom": "0.1.19",
    "xpath.js": "1.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.28.1",
    "electron-rebuild": "^1.1.4",
    "enzyme": "^2.3.0",
    "eslint": "^3.19.0",
    "eslint-config-rapid7": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^3.3.0",
    "mocha-lcov-reporter": "^1.2.0",
    "react-hot-loader": "^1.3.0",
    "should": "^11.2.1",
    "style-loader": "^0.17.0",
    "typings": "^2.1.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
